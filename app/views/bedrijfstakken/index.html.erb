<p id="notice"><%= notice %></p>

<h1>Bedrijfstakken</h1>


<table class="cmsTabel">
  <thead>
    <tr>
      <th>id</th>
      <th>Letter</th>
      <th>Naam</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bedrijfstakken.each do |bedrijfstak| %>
      <tr>
        <td><%= bedrijfstak.id %></td>
        <td><%= bedrijfstak.letter %></td>
        <td><%= bedrijfstak.naam %></td>
        <td><%= link_to 'Show', bedrijfstak %></td>
        <td><%= link_to 'Edit', edit_bedrijfstak_path(bedrijfstak) %></td>
        <td><%= link_to 'Destroy', bedrijfstak, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bedrijfstak', new_bedrijfstak_path %>
