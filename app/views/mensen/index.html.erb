<p id="notice"><%= notice %></p>

<h1>Mensen</h1>

<table class="cmsTabel" >
  <thead>
    <tr>
      <th>Naam</th>
      <th>Leeftijd</th>
      <th>Geslacht</th>
      <th>Kennisniveau</th>
      <th>Gezondheidsniveau</th>
      <th>Geluksniveau</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @mensen.each do |mens| %>
      <tr>
        <td><%= mens.naam %></td>
        <td><%= mens.leeftijd %></td>
        <td><%= mens.geslacht %></td>
        <td><%= mens.kennisniveau %></td>
        <td><%= mens.gezondheidsniveau %></td>
        <td><%= mens.geluksniveau %></td>
        <td><%= link_to 'Show', mens %></td>
        <td><%= link_to 'Edit', edit_mens_path(mens) %></td>
        <td><%= link_to 'Destroy', mens, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Mens', new_mens_path %>
